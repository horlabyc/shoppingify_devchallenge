es/http';\nimport Button from '../../shared/Button/button';\nimport ConfirmationDialog from '../../shared/ConfirmationDialog/confirmationDialog';\nimport Input from '../../shared/Input/input';\nimport Spinner from '../Spinner/spinner';\nimport './addNewItem.scss';\n\nexport interface AddNewItemProps {\n  \n}\n \nconst AddNewItem: React.FunctionComponent<AddNewItemProps> = () => {\n  const { register, handleSubmit, errors, setValue, reset} = useForm();\n  const [loading, setLoading] = useState(false);\n  const [buttonState, setButtonState] = useState<'idle' | 'loading'>('idle');\n  const [buttonDisabled, setButtonDisabled] = useState<boolean>(false);\n  const dispatch = useDispatch();\n  const { dispatch: dispatchContext } = useContext(AppContext);\n  const showShoppingList = () => {\n    dispatchContext({type: 'SET_RIGHT_SIDE_MENU_TYPE', payload: 'shoppingList'});\n  }\n  const onSubmit  = (data: any) => {\n    setButtonState('loading');\n    setButtonDisabled(true);\n    const payload = {\n      name: data.name,\n      category: data.category,\n      description: data.description,\n      extraNote: data.extraNote,\n      unitMeasure: data.unitMeasure\n    }\n    POST('items', payload).then((res) => {\n      setButtonState('idle');\n      setButtonDisabled(false);\n      dispatch(fetchItems());\n      reset();\n    })\n  };\n\n  const [categories, setCategories] = useState([])\n  useEffect(() => {\n    setLoading(true);\n    GET('categories').then(({data}) => {\n      // dispatch(setCategories(data.data));\n      setCategories(data.data);\n      setLoading(false);\n    })\n  }, [dispatch]);\n  return (  \n    <div className=\"addNewItem\">\n      <form onSubmit={handleSubmit(onSubmit)}>\n      <Input \n        name=\"name\"\n        defaultValue=\"\"\n        label=\"Name\"\n        type=\"text\"\n        placeholder=\"Enter a name\"\n        inputRef={register({  \n          required: {\n            value: true,\n            message: 'Name is required'\n          }\n        })}\n      />\n      { errors['name'] && <p className=\"inputError\">{ errors['name'].message }</p>}\n      <label>Description (Optional)</label>\n      <textarea   \n        name=\"description\"\n        rows={5}\n        cols={55}\n        defaultValue=\"\"\n        placeholder=\"Describe the item\"\n        ref={register()}\n      />\n      <label>Note (Optional)</label>\n      <textarea   \n        name=\"extraNote\"\n        rows={5}\n        cols={55}\n        defaultValue=\"\"\n        placeholder=\"Enter a note\"\n        ref={register()}\n      />\n      <Input \n        name=\"category\"\n        defaultValue=\"\"\n        label=\"Category\"\n        type=\"text\"\n        placeholder=\"Enter a category or select from list below\"\n        inputRef={register({  \n          required: {\n            value: true,\n            message: 'Category is required'\n          }\n        })}\n      />\n      { errors['category'] && <p className=\"inputError\">{ errors['category'].message }</p>}\n      {\n        loading ? <Spinner></Spinner> : \n        <ul className=\"categories\">\n          {\n            categories.map((category:any) => (\n              <li \n                className=\"category\" \n                key={category._id}\n                onClick={() => setValue('category', category.name, {shouldDirty: true, shouldValidate: true})}\n              >\n                {category.name}\n              </li>\n            ))\n          }\n        </ul>\n      }\n      <Input \n        name=\"unitMeasure\"\n        defaultValue=\"\"\n        label=\"Unit\"\n        type=\"text\"\n        placeholder=\"Unit (e.g kg)\"\n        inputRef={register()}\n      />\n      <div className=\"actions\">\n        <p onClick={showShoppingList}>cancel</p>\n        <Button type=\"submit\" action=\"Save\" variant=\"secondary\" className=\"actions__add\" state={buttonState} disabled={buttonDisabled}></Button>\n      </div>\n      </form>\n    </div>\n  );\n}\n \nexport default AddNewItem;","/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/components/MainContent/Header/header.tsx",["100","101","102"],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/components/MainContent/ShoppingItem/shoppingItem.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/routes/protectedRoute.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/pages/auth/login/login.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/pages/home/home.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/shared/MainLayout/mainLayout.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/components/ShoppingList/shoppingList.tsx",["103","104","105","106"],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/components/ShoppingList/Shopping-Item/shopping-item.tsx",["107"],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/services/http.ts",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/app/store.ts",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/shared/RightSideMenu/rightSideMenu.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/components/Spinner/spinner.tsx",[],"/Users/horlabyc/SoftwareLounge/Apps/React/Shoppingify/shoppingify/src/features/shoppingListSlice.ts",["108"],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"109","replacedBy":"113"},{"ruleId":"111","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":4,"column":6,"nodeType":"117","messageId":"118","endLine":4,"endColumn":15},{"ruleId":"115","severity":1,"message":"119","line":5,"column":10,"nodeType":"117","messageId":"118","endLine":5,"endColumn":23},{"ruleId":"115","severity":1,"message":"120","line":9,"column":8,"nodeType":"117","messageId":"118","endLine":9,"endColumn":2